<h1>Login</h1>
<br>
<div class="form-group">
  <div>
    <%= form_tag '/login' do %>
      <div class="col-sm-2">Email: </div>
      <div class="col-sm-10"><%= text_field_tag :email %></div>
      <div class="col-sm-2">Password: </div>
      <div class="col-sm-10"><%= password_field_tag :password %></div>
      <%= submit_tag "Submit" %>
    <% end %>
  </div>
</div>
